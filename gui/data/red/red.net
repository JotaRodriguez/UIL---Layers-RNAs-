 const{
	threads=4
	batch=100
	log="mi_red.log"
}


data {
	D1 [filename="training", binary]	
	D2 [filename="test", binary]
}

network mired {
	data tr D1
	data ts D2

	FI in

	F f1 [numnodes=24]
	FO out [classification]
	in->f1
	f1->out
}

script {
	//Normaliza los datos en cero y uno (0,1)	
	D1.div(255)
	D2.div(255)
	
	//porcentaje del factor de aprendizaje
	mired.mu = 0.01	

	//implementaci√≥n de ruido
	mired.noiser = 1.0
	mired.noisesd = 0.3

	 mired.drop=0.21


	mired.train(200)

} 	
 